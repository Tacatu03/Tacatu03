#include <stdio.h>
#include <stdlib.h>
#include <time.h>
//HAZ UN PROGRAMA QUE RELLENE UNA MATRIZ 3X3 DE 1 Y 0 AL AZAR
//Â¡pista! Utiliza 2 bucles de tipo for

int main(void) {
  int fil;
  int col;
  int numeros[4][4];
  int seed;
  seed=time(0);
  srand(seed);
  
  for(fil=0;fil<4;fil++){
    for(col=0;col<4;col++){
      numeros[fil][col]=(rand()%6)+1;
    }
  }
  printf("\nMATRIZ GUARDADA:");
  for(fil=0;fil<4;fil++){
    printf("\n");
    for(col=0;col<4;col++){
      printf(" %d",numeros[fil][col]);

    }
  }
  return 0;
}
