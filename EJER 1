#include <stdio.h>

int main() {
    int num1, num2, num3;

    // Solicitar al usuario que ingrese tres números enteros
    printf("Ingrese el primer número entero: ");
    scanf("%d", &num1);

    printf("Ingrese el segundo número entero: ");
    scanf("%d", &num2);

    printf("Ingrese el tercer número entero: ");
    scanf("%d", &num3);

    // Comparar los números para encontrar el mayor
    int mayor = num1;

    if (num2 > mayor) {
        mayor = num2;
    }

    if (num3 > mayor) {
        mayor = num3;
    }

    // Mostrar el resultado
    printf("El número mayor es: %d\n", mayor);

    return 0;
}
